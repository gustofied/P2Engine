# P2Engine Environment Configuration

CONFIG_FILE=config/config.json

# OpenAI API Configuration (REQUIRED)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=""

# Canton/Ledger Configuration
# Set to false to run without Canton (recommended for quick start)
LEDGER_ENABLED=true


# If you want to use a specific Canton version (when LEDGER_ENABLED=true)
# CANTON_VERSION=2.8.11

# Logging Configuration
# LOG_LEVEL=INFO
# WORKER_LOG_LEVEL=INFO
# LITELLM_LOG_LEVEL=error

# Redis Configuration (optional - defaults are usually fine)
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_DB=0

# Deduplication Policy for tool calls
# Options: none, penalty, strict
DEDUP_POLICY=none

# Artifact Storage Driver
# Options: fs (filesystem), s3
ARTIFACT_DRIVER=fs

# For development/debugging
# LOG_TO_CONSOLE=1
# LITELLM_LOG_LEVEL=debug
DAML_PACKAGE_ID=fc1a7f2a0ce50d925e7f4e9b1801f95351143dfb9bff3bdd32d6dc2a6c67664b
